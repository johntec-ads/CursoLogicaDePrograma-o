Algoritmo "EnquantoCrescente"

Var
   x , y : inteiro

Inicio

   escreval("Digite dois numeros: ")
   leia(x,y)

   enquanto  x <> y faca
      se x > y entao
         escreval("Descrescente")
      senao
         escreval("Crescente")
      fimse
      escreval("Digite dois numeros: ")
      leia(x,y)
   fimenquanto



Fimalgoritmo