Algoritmo "CondicionalDardo"

Var
   a , b , c : real


Inicio
      escreval("Digite as tres distancias: ")
      leia(a , b ,c )
       se a > b entao
       escreval("MAIOR DISTANCIA = ",a:4:2)
       senao
         se b > c entao
         escreval("MAIOR DISTANCIA = ",b:4:2)
         senao
         escreval("MAIOR DISTANCIA = " ,c:4:2)
         fimse
       fimse


Fimalgoritmo